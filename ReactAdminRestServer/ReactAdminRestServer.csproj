<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>React-Admin-RestServer</PackageId>
    <Authors>Zachry Baker</Authors>
    <Company />
    <Product>React-Admin-RestServer</Product>
    <Description>A .Net Core REST API builder that auto generates EF entities automapped to CRUD models and exposes them in a react-admin - compatible REST dialect.</Description>
    <Copyright>Zachry Baker</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/zachrybaker/React-Admin-RestServer</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>react-admin, EFCore generator, autogenerated DAL, REST API, automapperu</PackageTags>
    <Version>1.0.2</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyName>React-Admin-RestServer</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="generation.yml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="generation.yml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="10.0.0" />
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="8.0.1" />
    <PackageReference Include="LinqKit" Version="1.1.17" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.8" />
    <PackageReference Include="System.Linq.Dynamic.Core" Version="1.2.2" />
  </ItemGroup>

</Project>
